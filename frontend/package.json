{
    "name": "organizer-shopping-list-frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@apollo/client": "^3.5.6",
        "@material-ui/core": "^4.12.3",
        "@mui/icons-material": "^5.1.1",
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "@types/jest": "^26.0.15",
        "@types/node": "^12.0.0",
        "graphql": "^15.8.0",
        "react": "^18.0.0-alpha-ee069065d-20211105",
        "react-dom": "^18.0.0-alpha-ee069065d-20211105",
        "react-scripts": "^5.0.0",
        "typescript": "^4.1.2",
        "web-vitals": "^1.0.1"
    },
    "devDependencies": {
        "@emotion/react": "^11.7.1",
        "@emotion/styled": "^11.6.0",
        "@graphql-codegen/add": "^3.1.0",
        "@graphql-codegen/cli": "^2.3.0",
        "@graphql-codegen/typescript": "^2.4.1",
        "@graphql-codegen/typescript-operations": "^2.2.1",
        "@graphql-codegen/typescript-react-apollo": "^3.2.2",
        "@svgr/webpack": "^6.1.2",
        "@trivago/prettier-plugin-sort-imports": "^3.1.1",
        "@types/react": "^17.0.34",
        "@types/react-dom": "^17.0.11",
        "css-loader": "^5.1.2",
        "css-modules-typescript-loader": "^4.0.1",
        "customize-cra": "^1.0.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^7.2.0",
        "eslint-config-react-app": "^6.0.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "husky": "^7.0.4",
        "lint-staged": "^11.1.2",
        "prettier": "^2.2.1",
        "react-app-rewired": "^2.1.8",
        "sass": "^1.32.8",
        "sass-loader": "^10.1.1",
        "style-loader": "^2.0.0",
        "stylelint": "^13.13.1",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-recommended-scss": "^4.3.0",
        "stylelint-config-standard": "^22.0.0",
        "stylelint-prettier": "^1.2.0",
        "stylelint-scss": "^3.21.0",
        "svg-url-loader": "^7.1.1",
        "ts-loader": "^8.0.18",
        "url-loader": "^4.1.1"
    },
    "scripts": {
        "remove_husky": "rimraf .husky",
        "postinstall": "npm run remove_husky && cd ../ && husky install frontend/.husky && husky add frontend/.husky/pre-commit 'cd frontend && npm run pre-commit' && husky add frontend/.husky/pre-push 'cd frontend && npm run pre-push' && cd frontend && npm run generateGraphQLHooks",
        "pre-commit": "lint-staged",
        "pre-push": "npm run check_all",
        "start": "react-app-rewired start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "eslint_check": "eslint 'src/**/*.{js,*.js,ts,*.ts,tsx,*.tsx}'",
        "stylelint_check": "stylelint 'src/**/*.{scss,*.scss,css,*.css}'",
        "prettier_check": "prettier --check 'src/**/*.{js,*.js,ts,*.ts,tsx,*.tsx,scss,*.scss,css,*.css,json}'",
        "check_all": "npm run prettier_check && npm run eslint_check && npm run stylelint_check",
        "eslint_fix": "eslint 'src/**/*.{js,*.js,ts,*.ts,tsx,*.tsx}' --fix",
        "stylelint_fix": "stylelint 'src/**/*.{scss,*.scss,css,*.css}' --fix",
        "prettier_fix": "prettier --write 'src/**/*.{js,*.js,ts,*.ts,tsx,*.tsx,scss,*.scss,css,*.css,json}'",
        "fix_all": "npm run prettier_fix && npm run eslint_fix && npm run stylelint_fix",
        "generateGraphQLHooks": "graphql-codegen --require dotenv/config --config codegen.yml",
        "generateGraphQLHooks:watch": "graphql-codegen --require dotenv/config --config codegen.yml --watch"
    },
    "lint-staged": {
        "src/**/*.{js,*.js,ts,*.ts,tsx,*.tsx,json}": [
            "prettier --write",
            "eslint --fix"
        ],
        "src/**/*.{scss,*.scss,css,*.css,*.scss.d.ts,.scss.d.ts}": [
            "stylelint --fix"
        ]
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
